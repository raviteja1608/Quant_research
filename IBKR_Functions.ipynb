    "\n",
    "@dataclass\n",
    "class ContractDetails:\n",
    "    \"\"\"Contract details structure\"\"\"\n",
    "    symbol: str\n",
    "    sec_type: str\n",
    "    exchange: str\n",
    "    currency: str\n",
    "    expiry: str = ''\n",
    "    strike: float = 0.0\n",
