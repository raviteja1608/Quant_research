    "    def place_order(self, contract_details: ContractDetails, \n",
    "                   order_details: OrderDetails) -> Optional[int]:\n",
    "        \"\"\"\n",
    "        Place an order\n",
    "        \n",
    "        Args:\n",
    "            contract_details: Contract details\n",
    "            order_details: Order details\n",
    "            \n",
    "        Returns:\n",
