    "class TimeInForce(Enum):\n",
    "    DAY = 'DAY'\n",
    "    GOOD_TILL_CANCEL = 'GTC'\n",
    "    IMMEDIATE_OR_CANCEL = 'IOC'\n",
    "    FILL_OR_KILL = 'FOK'\n",
    "    GOOD_TILL_DATE = 'GTD'\n",
    "\n",
    "# Action types\n",
    "class ActionType(Enum):\n",
    "    BUY = 'BUY'\n",
